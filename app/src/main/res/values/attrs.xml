<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialTextView">
        <attr name="tipo" format="enum">
            <enum name="Titulo"         value="0"/>
            <enum name="Subtitulo"      value="1"/>
            <enum name="Descripcion"    value="2"/>
            <enum name="Indicacion"     value="3"/>
            <enum name="Text"           value="4"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="MaterialButton">
        <attr name="intercepterOnclick" format="boolean"/>
        <attr name="buttonRippleColor2" format="color"/>
        <attr name="buttonNormalColor" format="color"/>
        <attr name="selectedTextColor" format="color"/>
        <attr name="cornerRadius" format="dimension"/>
        <attr name="buttonStrokeColor" format="color"/>
        <attr name="overBackground" format="reference"/>
        <attr name="buttonShape"/>

        <attr name="filling_color" format="color"/>
        <attr name="outline_color" format="color"/>

    </declare-styleable>
    <attr name="buttonShape" format="enum">
        <enum name="rectangle" value="0"/>
        <enum name="oval" value="1"/>
    </attr>
    <declare-styleable name="MaterialValidationEditText">
        <attr name="formato" format="enum">
            <enum name="email" value="0"/>
            <enum name="password" value="1"/>
            <enum name="phone" value="2"/>
            <enum name="zipcode" value="3"/>
            <enum name="text" value="4"/>
            <enum name="number" value="5"/>
            <enum name="celphone" value="6"/>
            <enum name="pwtext" value="7"/>
            <enum name="idptext" value="8"/>
        </attr>
        <attr name="hintText" format="string"/>
        <attr name="hintColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="defaultIcon" format="reference"/>
        <attr name="maxLength" format="integer"/>
        <attr name="minSizeRequired" format="integer"/>
        <attr name="android:gravity"/>
        <attr name="maxLines" format="integer"/>
        <attr name="isSingleLine" format="boolean"/>
        <attr name="isTextEnabled" format="boolean"/>
        <attr name="android:inputType"/>
        <attr name="android:textSize"/>
        <attr name="focusableInTouchMode" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CustomBottomSheetBehavior">
        <attr name="anchorPoint" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="StatusCupoColors">
        <attr name="bgColor" format="color"/>
        <attr name="firstColor" format="color" />
        <attr name="secondColor" format="color" />
        <attr name="errorColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="startAngle" format="integer"/>
        <attr name="percent" format="integer"/>
        <attr name="parts" format="integer"/>
    </declare-styleable>

    <declare-styleable name="PinKeyboardView">
        <attr name="numDigits" format="integer" />
        <attr name="digitWidth" format="dimension" />
        <attr name="digitHeight" format="dimension" />
        <attr name="digitSpacing" format="dimension" />
        <attr name="digitBackground" format="reference|color" />
        <attr name="digitTextSize" format="dimension" />
        <attr name="digitTextColor" format="color" />
        <attr name="digitElevation" format="dimension" />
        <attr name="mask" format="string" />
        <attr name="pinBorderColor" format="color" />
        <attr name="accentWidth" format="dimension" />
        <!-- Accent type, default 'character' -->
        <attr name="accentType" format="enum">
            <enum name="none" value="0" />
            <enum name="all" value="1" />
            <enum name="character" value="2" />
        </attr>
        <!-- Input type -->
        <attr name="pinInputType" format="enum">
            <enum name="text" value="1" />
            <enum name="number" value="2" />
        </attr>
        <attr name="android:textStyle">bold</attr>
        <!-- Only draw accent when View has focus -->
        <attr name="accentRequiresFocus" format="boolean" />
    </declare-styleable>

</resources>